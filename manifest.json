{
  "manifest_version": 2,
  "name": "SWAC - Steam Workshop Auto-Collection",
  "version": "1.0",
  "description": "Automatically subscribes to items in a Steam Collection.",
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "https://steamcommunity.com/*"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
